{
  "label_setup": {
    "version": 1,
    "labels": [ 
      {
        "__merge__": true
      },
      {
        "name": "add questions",
        "description": "add somet questions",
        "color": "#006b75"
      },
      {
        "name": "bug",
        "description": "Something isn't working",
        "color": "#d73a4a"
      }
      
    ]
  },
  "weekly_report": {
    "version": 1,
    "generateTime": "0 0 12 * * 1"
  },
  "role": {
    "version": 1,
    "roles": [
      {
        "name": "committer",
        "description": "Committer of the project",
        "users": [ "QuakeWang", "freedom3219", "jaydenhd" ],
        "commands": [ "/approve" ]
      },
      {
        "name": "replier",
        "description": "Replier is responsible for reply issues in time",
        "users": [ "QuakeWang", "freedom3219", "jaydenhd" ],
        "commands": []
      },
      {
        "name": "notauthor",
        "description": "Not author of the issue or pull",
        "users": [],
        "commands": [ "/approve" ]
      },
      {
        "name": "anyone",
        "description": "Anyone",
        "users": [],
        "commands": [ "/self-assign" ]
      }
    ]
  },
  "command": {
    "version": 1,
    "commands": [
      {
        "name": "/approve",
        "scopes": [ "review", "review_comment", "pull_comment" ]
      }
    ]
  },
  "approve": {
    "version": 1
  },
  "auto_merge": {
    "version": 1,
    "sched": "0 */5 * * * *"
  },
  "issue_reminder": {
    "version": 1
  },
  "auto_label": {
    "version": 1
  },
  "self_assign": {
    "version": 1
  },
  "sql_verified": {
    "version": 1
  },
  "auto_update_report": {
    "version": 1
  },
  "auto_update_contribution": {
    "version": 1
  },
  "pr_uml_renderer": {
    "version": 1
  },
  "pr_file_label": {
    "version": 1,
    "rules": [
      {
        "files": [
          "docs/.+"
        ],
        "label": "pull/website"
      },
      {
        "files": [
          "%.github/hypertrons%.json",
          "%.github/hypertrons%-components/.+"
        ],
        "label": "pull/hypertrons"
      }
    ]
  }
}
